  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-boost": "^0.1.20",
    "apollo-cache-inmemory": "^1.3.9",
    "apollo-client": "^2.4.5",
    "apollo-client-preset": "^1.0.8",
    "apollo-link": "^1.2.3",
    "apollo-link-context": "^1.0.7",
    "apollo-link-http": "^1.5.5",
    "apollo-link-state": "^0.4.2",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "date-fns": "^1.29.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "history": "^4.7.2",
    "netlify-auth-providers": "^1.0.0-alpha5",
    "ramda": "^0.25.0",
    "react": "^16.7.0-alpha.2",
    "react-apollo": "^2.1.2",
    "react-apollo-hooks": "^0.1.6",
    "react-dom": "^16.7.0-alpha.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.1",
    "react-virtualized": "^9.21.0",
    "recompose": "^0.30.0",
    "shallow-equal": "^1.0.0",
    "styled-components": "^4.1.1",
    "styled-system": "^2.2.1",
    "utility-types": "^2.1.0"
  },
  "resolutions": {
    "apollo-codegen/graphql": "^14.0.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.2",
    "@storybook/addon-links": "^3.4.2",
    "@storybook/react": "^3.4.2",
    "@types/classnames": "^2.2.6",
    "@types/graphql": "^14.0.3",
    "@types/ramda": "^0.25.41",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-virtualized": "^9.18.7",
    "@types/recompose": "^0.27.0",
    "@types/styled-components": "^4.1.0",
    "@types/styled-system": "^3.0.6",
    "apollo-codegen": "^0.20.2",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "prettier": "^1.15.1",
    "prettier-standard": "^8.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "heroku-postbuild": "npm run build",
    "lint": "tslint-config-prettier-check ./tslint.json && tslint src/**/*.tsx src/**/*.ts",
    "fix": "prettier-standard src/**/*.ts src/**/*.tsx",
    "codegen": "apollo-codegen generate ./src/data/**/*.ts --schema ./src/data/schema.json --target typescript --output ./src/data/types.ts"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier-standard",
      "git add"
    ],
    "*.tsx": [
      "prettier-standard",
      "git add"
    ]
  },
  "apollo": {
    "client": {
      "name": "bida-app",
      "service": {
        "name": "github-service",
        "url": "https://api.github.com/graphql"
      }
    }
  }
}
