    "apollo-client": "^2.4.6",
    "apollo-client-preset": "^1.0.8",
    "apollo-link": "^1.2.3",
    "apollo-link-context": "^1.0.7",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "^1.5.5",
    "apollo-link-rest": "^0.6.0",
    "apollo-link-state": "^0.4.2",
    "apollo-utilities": "^1.0.26",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "date-fns": "^1.29.0",
    "deep-equal": "^1.0.1",
    "graphql": "^14.0.2",
    "graphql-anywhere": "^4.1.23",
    "graphql-tag": "^2.10.0",
    "history": "^4.7.2",
    "netlify-auth-providers": "^1.0.0-alpha5",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "react": "^16.7.0-alpha.2",
    "react-apollo": "^2.1.2",
    "react-cache": "^2.0.0-alpha.1",
    "react-dom": "^16.7.0-alpha.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.1",
    "react-virtualized": "^9.21.0",
    "recompose": "^0.30.0",
    "semver": "^5.6.0",
    "shallow-equal": "^1.0.0",
    "styled-components": "^4.1.1",
    "styled-system": "^2.2.1",
    "utility-types": "^2.1.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.2",
    "@storybook/addon-links": "^3.4.2",
    "@storybook/react": "^3.4.2",
    "@types/classnames": "^2.2.6",
    "@types/deep-equal": "^1.0.1",
    "@types/graphql": "^14.0.3",
    "@types/prop-types": "^15.5.6",
    "@types/ramda": "^0.25.41",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-virtualized": "^9.18.7",
    "@types/recompose": "^0.27.0",
    "@types/styled-components": "^4.1.0",
    "@types/styled-system": "^3.0.6",
    "@types/uuid": "^3.4.4",
    "apollo": "^2.1.3",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "prettier": "^1.15.1",
    "prettier-standard": "^8.0.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "lint": "tslint-config-prettier-check ./tslint.json && tslint src/**/*.tsx src/**/*.ts",
    "fix": "prettier-standard src/**/*.ts src/**/*.tsx",
    "codegen": "rimraf src/__generated-types* && apollo codegen:generate __generated-types --target typescript --includes=src/**/* --globalTypesFile src/data/__generated-types.ts"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier-standard",
      "git add"
    ],
    "*.tsx": [
      "prettier-standard",
      "git add"
    ]
  },
  "apollo": {
    "client": {
      "name": "bida-app",
      "service": {
        "name": "github-service",
        "localSchemaFile": "src/config/github-schema.json"
      }
    }
  }
}
