{
  "name": "bida-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "run-p build:app build:lambda && yarn build:analysis",
    "build:analysis": "mkdir build -p build/.dev && source-map-explorer 'build/static/js/*.js' --html > build/.dev/bundle-analysis.html",
    "build:app": "tsc -p tsconfig.production.json && NODE_ENV=production react-scripts build",
    "build:lambda": "netlify-lambda build src/lambda --config ./webpack.functions.js",
    "codegen": "apollo codegen:generate __generated-types --target typescript --includes=src/**/* --globalTypesFile src/data/__generated-types.ts",
    "dev-api": "onchange -i -k '**/*.js' '**/*.ts' -- ts-node -r dotenv/config -O '{ \"isolatedModules\": false, \"module\": \"commonjs\" }' serve-api.ts",
    "fix": "prettier-standard src/**/*.ts src/**/*.tsx",
    "lint": "tslint-config-prettier-check ./tslint.json && tslint src/**/*.tsx src/**/*.ts",
    "pr": "yarn pull-request",
    "pull-request": "node ./scripts/pull-request.js",
    "qa": "tsc -p tsconfig.production.json && yarn test",
    "start": "run-p start:**",
    "start:app": "react-scripts start",
    "start:lambda": "netlify-lambda serve src/lambda --config ./webpack.functions.js",
    "storybook:build": "build-storybook -s public",
    "storybook:start": "start-storybook -p 9009 -s public",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "node ./scripts/stashed.js 'yarn qa'"
    }
  },
  "lint-staged": {
    "*.md": [
      "node ./scripts/toc.js",
      "git add README.md"
    ],
    "*.{ts,tsx}": [
      "prettier-standard",
      "tslint --fix",
      "stylelint --fix",
      "git add"
    ],
    "package.json": [
      "yarn sort-package-json",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@rehooks/local-storage": "^1.4.0",
    "apollo-cache": "^1.2.1",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-client-preset": "^1.0.8",
    "apollo-link": "^1.2.11",
    "apollo-link-context": "^1.0.17",
    "apollo-link-dedup": "^1.0.18",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "apollo-link-schema": "^1.2.2",
    "apollo-link-state": "^0.4.2",
    "apollo-server-lambda": "^2.4.8",
    "apollo-utilities": "^1.2.1",
    "aws-lambda": "^0.1.2",
    "dataloader": "^1.4.0",
    "debug": "^4.1.1",
    "deep-equal": "^1.0.1",
    "graphql": "^14.2.1",
    "graphql-anywhere": "^4.1.23",
    "graphql-playground-middleware-lambda": "^1.7.13",
    "graphql-resolvers": "^0.3.2",
    "graphql-tag": "^2.10.0",
    "graphql-tools": "^4.0.4",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "mem": "^4.3.0",
    "memory-cache": "^0.2.0",
    "netlify-auth-providers": "^1.0.0-alpha5",
    "prop-types": "^15.6.2",
